{
  "name": "jss-sample-nextjs-personalization",
  "description": "Application utilizing Sitecore JavaScript Services and Next.js",
  "version": "19.0.0-canary.1",
  "private": true,
  "config": {
    "appName": "JssNextPersonalizationWeb",
    "rootPlaceholders": [
      "jssnextpersonalizationweb-main"
    ],
    "sitecoreConfigPath": "/App_Config/Include/zzp",
    "graphQLEndpointPath": "/sitecore/api/graph/edge",
    "trackingEndpointPath": "/api/track",
    "personalizationDecisionsEndpointPath": "/api/personalization/decision",
    "language": "en"
  },
  "engines": {
    "node": ">=8.1",
    "npm": ">=5.6.0",
    "yarn": "yarn is not supported, please use npm"
  },
  "author": {
    "name": "Sitecore Corporation",
    "url": "https://jss.sitecore.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sitecore/jss.git"
  },
  "bugs": {
    "url": "https://github.com/sitecore/jss/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@sitecore-jss/sitecore-jss-nextjs": "^19.0.0-canary.15",
    "@sitecore-jss/sitecore-jss-tracking": "^19.0.0-canary.15",
    "bootstrap": "^4.3.1",
    "next": "^10.0.3",
    "next-localization": "^0.10.0",
    "nprogress": "~0.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@sitecore-jss/sitecore-jss-cli": "^19.0.0-canary.1",
    "@types/chai": "^4.2.3",
    "@types/chai-spies": "^1.0.3",
    "@types/enzyme": "^3.10.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^14.6.4",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/sinon": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "axios": "^0.21.1",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "chalk": "~2.4.2",
    "chokidar": "~3.1.1",
    "constant-case": "^3.0.4",
    "cross-env": "~6.0.3",
    "enzyme": "^3.10.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-yaml": "^0.2.0",
    "http-proxy-middleware": "^1.2.0",
    "jsdom": "15.2.1",
    "jsdom-global": "3.0.2",
    "mocha": "^8.1.3",
    "next-transpile-modules": "^7.3.0",
    "npm-run-all": "~4.1.5",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.7.0",
    "ts-node": "^9.0.0",
    "ts-sinon": "^2.0.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "jss": "jss",
    "lint": "eslint ./src/**/*.tsx ./src/**/*.ts ./scripts/**/*.ts",
    "bootstrap": "ts-node --project tsconfig.scripts.json scripts/bootstrap.ts",
    "build": "npm-run-all --serial bootstrap next:build",
    "next:build": "cross-env next build",
    "next:dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "next:start": "cross-env next start",
    "start": "npm-run-all --serial bootstrap next:build next:start",
    "start:connected": "npm-run-all --serial bootstrap --parallel next:dev start:watch-components",
    "start:watch-components": "ts-node --project tsconfig.scripts.json scripts/generate-component-factory.ts --watch",
    "test": "cross-env TS_NODE_PROJECT='./tsconfig.scripts.json' mocha --require ts-node/register --require tsconfig-paths/register --require scripts/bootstrap.ts ./src/tests/jsdom-setup.ts ./src/tests/shim.ts ./src/tests/enzyme-setup.ts \"./src/**/*.test.ts\" \"./src/**/*.test.tsx\""
  }
}
